{
    "CORSRules": [
        {
            "ID": "JuiceVaultCORS",
            "AllowedHeaders": ["*"],
            "AllowedMethods": ["GET", "PUT", "POST", "DELETE", "HEAD"],
            "AllowedOrigins": [
                "http://localhost:3000",
                "http://localhost:3001", 
                "http://localhost:*",
                "https://*"
            ],
            "ExposeHeaders": [
                "ETag",
                "x-amz-server-side-encryption",
                "x-amz-request-id",
                "x-amz-id-2",
                "x-amz-checksum-crc32"
            ],
            "MaxAgeSeconds": 3600
        }
    ]
}